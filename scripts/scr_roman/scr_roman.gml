function scr_roman(argument0) {
    // turns into numeral

    if (argument0 == 1) {
        return "I";
    }
    if (argument0 == 2) {
        return "II";
    }
    if (argument0 == 3) {
        return "III";
    }
    if (argument0 == 4) {
        return "IV";
    }
    if (argument0 == 5) {
        return "V";
    }
}
